{
	"name": "DF_TitleMaster_ADLS_SQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TitleMaster",
						"type": "DatasetReference"
					},
					"name": "ADLSTitleMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Title_data",
						"type": "DatasetReference"
					},
					"name": "SINKSQLTitleMaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ADLSTitleMaster",
				"ADLSTitleMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SINKSQLTitleMaster"
			]
		}
	}
}